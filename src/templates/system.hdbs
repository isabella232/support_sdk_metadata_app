<div id="sdk_metadata_app">
  <div class="row">
    <div id="device"></div>
  </div>
  <div class="row">
    <div class="row">
      <div class="user-info">
        <div class="half-left">
          <h6>IP address</h6>
          <p>{{system.ip_address}}</p>
        </div>
        <div class="half-right">
          <h6>Location</h6>
          <p>{{system.location}}</p>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="sdk-version">
    <p>{{sdk}}</p>
</div>
